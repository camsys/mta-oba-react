{
  "name": "mta-oba-react",
  "version": "0.5.23-SNAPSHOT",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production webpack --mode production --config webpack.config.js",
    "dev-start": "NODE_ENV=development webpack serve --config webpack.config.js",
    "dev-start:clean": "npx madge --circular src; rm -rf dist .webpack-cache .tsbuildinfo; webpack serve --config webpack.config.js",
    "serve": "npx serve -l 8080 dist",
    "acceptTracking": "node logger.js",
    "build-css": "sass src/css:public/css && postcss public/css/*.css --use tailwindcss autoprefixer --no-map -d public/css/",
    "watch-sass": "sass --watch src/css:tmp/css",
    "watch-prefix": "onchange \"tmp/css/*.css\" -- npx postcss {{changed}} --use tailwindcss autoprefixer --no-map -d public/css/",
    "dev-css": "npm run build-css && concurrently \"npm run watch-sass\" \"npm run watch-prefix\"",
    "dev": "concurrently \"npm run dev-start:clean\" \"npm run dev-css\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.24.7",
    "@svgr/webpack": "^8.1.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.24",
    "babel-loader": "^9.1.3",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^9.2.1",
    "css-loader": "^6.10.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "onchange": "^7.1.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "react-router-dom": "^6.24.1",
    "sass": "^1.97.3",
    "style-loader": "^3.3.4",
    "svg-inline-loader": "^0.8.2",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.5.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "copy-webpack-plugin": "^13.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dompurify": "^3.2.6",
    "express": "^5.1.0",
    "js-cookie": "^3.0.5",
    "log4js": "^6.9.1",
    "loglevel": "^1.9.2",
    "query-string": "^9.0.0",
    "react": "^18.3.1",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-leaflet-google-layer": "^2.2.0",
    "react-router": "^6.23.1",
    "react-select": "^5.8.0",
    "react-svg": "^16.1.33",
    "uuid": "^11.1.0"
  }
